const N = 10;
var
a: array [1..N] of char;
c: char;
i, p, l : Integer;

begin

  readln(p)
  l:= 0;
  
  While Not Eoln do  //потоковый ввод строки более 255 символов
    begin
      read(c);
      inc(l); //l:=l+l, чтобы заполнялась ячейки
      readln(a[l]);
    end;
  
  readln;
  
  for i:= 1 to l do 
    begin
     write(a[i]);
     writeln;
     readln;
    end;
    
end.
